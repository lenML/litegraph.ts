{
    "name": "@litegraph-ts/core",
    "version": "0.2.18",
    "description": "A graph node editor similar to PD or UDK Blueprints. It works in an HTML5 Canvas and allows to export graphs to be included in applications.",
    "source": "src/index.ts",
    "types": "src/index.ts",
    "main": "src/index.ts",
    "private": false,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "jest",
        "prettier": "npx prettier --write src/**/*.* css/**/*.*",
        "lint": "npx eslint src",
        "lint:fix": "npx eslint --fix ./src/**/*.ts",
        "prepack": "rm -rf dist && pnpm run build",
        "build:docs": "typedoc --skipErrorChecking"
    },
    "author": "jagenjo",
    "license": "MIT",
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lenML/litegraph.ts.git"
    },
    "files": [
        "dist",
        "css/litegraph.css",
        "src"
    ],
    "bugs": {
        "url": "https://github.com/lenML/litegraph.ts/issues"
    },
    "homepage": "https://github.com/lenML/litegraph.ts#readme",
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/uuid": "^9.0.1",
        "eslint": "^8.37.0 ",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.5.0",
        "prettier": "^3.2.5",
        "typedoc": "^0.25.13",
        "typescript": "^5.0.3",
        "vite": "^4.2.1",
        "vite-plugin-checker": "^0.5.6",
        "vite-plugin-dts": "^2.2.0"
    },
    "dependencies": {
        "uuid": "^9.0.0"
    },
    "keywords": [
        "graph",
        "editor",
        "canvas",
        "webgl",
        "workflow",
        "litegraph",
        "litegraph.js",
        "litegraph.ts",
        "litegraph-ts",
        "litegraph.ts"
    ]
}
